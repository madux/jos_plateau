    <!-- Journals for Ministries -->

    <record id="journal_board_commission_bank" model="account.journal">
        <field name="name">Board Commission - Bank</field>
        <field name="type">bank</field>
        <field name="code">BC-BNK</field>
        <field name="default_account_id" ref="company_memo.account_account_mda_board_commission_bank_cash"/>
        <field name="allowed_branch_ids" eval="[(6,0,[
        ref(company_memo.mda_board_commission),
        ])]"/>
    </record>
    <record id="journal_board_commission_expense" model="account.journal">
        <field name="name">Board Commission - Expense</field>
        <field name="type">purchase</field>
        <field name="code">BC-EXP</field>
        <field name="default_account_id" ref="company_memo.account_account_mda_board_commission_expense"/>
        <field name="allowed_branch_ids" eval="[(6,0,[
        ref(company_memo.mda_board_commission),
        ])]"/>
    </record>
    <record id="journal_board_commission_sale" model="account.journal">
